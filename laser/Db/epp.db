############ Helper records ##########

# Attenuation
record(calc, "$(P)EPP:Throughput-1:sRdV") {
   field(SCAN, "$(SCAN)")
   field(DESC, "Throughput")
   field(EGU, "%")
   field(PREC, "2")
   field(INPA, "$(P)EPP:Picker-1:sRdV")
   field(CALC, "A=0?100:100*(A-1)/A")
}

